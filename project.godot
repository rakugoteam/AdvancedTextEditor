}, {
"base": "Button",
"class": "EmojiButton",
"language": "GDScript",
"path": "res://addons/emojis-for-godot/nodes/EmojiButton.gd"
}, {
"base": "TextureRect",
"class": "EmojiIcon",
"language": "GDScript",
"path": "res://addons/emojis-for-godot/nodes/EmojiIcon.gd"
}, {
"base": "Object",
"class": "Emojis",
"language": "GDScript",
